CREATE TABLE TodoEntity(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    description TEXT,
    due_date_ms INTEGER,
    added_at_ms INTEGER NOT NULL,
    edited_at_ms INTEGER
);

insertTodo:
INSERT INTO TodoEntity(title, description, due_date_ms, added_at_ms)
VALUES (?,?,?,?);

selectAllTodos:
SELECT TodoEntity.*
FROM TodoEntity;