CREATE TABLE TodoEntity(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    description TEXT,
    due_date_time INTEGER,
    added_at INTEGER NOT NULL,
    edited_at INTEGER
);

insertTodo:
INSERT INTO TodoEntity(title, description, due_date_time, added_at)
VALUES (?,?,?,?);

selectAllTodos:
SELECT TodoEntity.*
FROM TodoEntity;